generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Progresso {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  usuario   String
  fase      Int
  pontuacao Int
  criadoEm  DateTime @default(now())
}


